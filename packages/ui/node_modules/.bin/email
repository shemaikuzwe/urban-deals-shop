#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/react-email/dist/cli/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/react-email/dist/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/react-email/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/react-email/dist/cli/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/react-email/dist/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules/react-email/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/react-email@3.0.2_react-dom@19.2.3_react@19.2.3__react@19.2.3/node_modules:/home/shema/apps/personal/ecommerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/cli/index.js" "$@"
fi
